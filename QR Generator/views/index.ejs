    <!-- input field -->
    <div class="container-fluid px-1 py-5 mx-auto">
      <div class="row d-flex justify-content-center">
        <div class="col-xl-7 col-lg-8 col-md-9 col-11 text-center">
          <h3>Enter Your Data To Generate Qr Code</h3>

          <div class="card">
            <!--FORM-->
            <form class="form-card" action="/" method="post">
              <div class="row justify-content-between text-left">
                <div class="form-group col-sm-6 flex-column d-flex">
                  <label class="form-control-label px-3"
                    >First Name<span class="text-danger"> *</span></label
                  >
                  <input
                    type="text"
                    id="fname"
                    name="FNAME"
                    placeholder="Enter your First Name"
                    onblur="validate(1)"
                  />

                 
                  <label class="form-control-label px-3"
                    >Roll No<span class="text-danger"> *</span></label
                  >
                  <input
                    type="number"
                    id="Roll"
                    name="ROLL"
                    placeholder="Enter your Roll No"
                    onblur="validate(1)"
                  />
                  <label class="form-control-label px-3"
                    >Phone Number<span class="text-danger"> *</span></label
                  >
                  <input
                    type="number"
                    id="number"
                    name="PNUMBER"
                    placeholder="Enter your Phone Number"
                    onblur="validate(1)"
                  />
                </div>
                <div class="form-group col-sm-6 flex-column d-flex">
                  <label class="form-control-label px-3"
                    >Last name<span class="text-danger"> *</span></label
                  >
                  <input
                    type="text"
                    id="lname"
                    name="LNAME"
                    placeholder="Enter your last name"
                    onblur="validate(2)"
                  />
                  <label class="form-control-label px-3"
                    >SID<span class="text-danger"> *</span></label
                  >
                  <input
                    type="text"
                    id="SID"
                    name="SID"
                    placeholder="Enter your SID"
                    onblur="validate(2)"
                  />
                   <label class="branch-box">Select Branch<span class="text-danger"> *</span></label
                  >
              <select  name="BRANCH" id='select1' class="form-select form-control-label px-4" aria-label="Default select example" >
                            <option selected>Select Branch</option>
                            <option value="COMPS">COMPS</option>
                            <option value="CS&E">CS&E</option>
                            <option value="ECS">ECS</option>
                            <option value="ENTC">ENTC</option>
                            <option value="MME">MME</option>
                            <option value="CIVIL">CIVIL</option>
                            <option value="AI&DS">AI&DS</option>
                            <option value="Mechanical">Mechanical</option>
                            <option value="AIML">AIML</option>
                            <option value="ELEX">ELEX</option>
                            <option value="IOT">IOT</option>
                            <option value="E&TC">E&TC</option></select>
              </div>
              <div class="row justify-content-between text-left">
                
              <div class="row justify-content-center">
                <div class="form-group col-sm-6">
                  <button type="submit" class="btn-block btn-success">
                    Generate QR CODE
                  </button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
      crossorigin="anonymous"
    ></script>

